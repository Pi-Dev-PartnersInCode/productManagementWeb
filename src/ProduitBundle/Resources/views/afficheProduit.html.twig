{% extends 'base.html.twig' %}
{% block body %}

    <table >
        <thead>
        <tr>
            <th>Id</th>
            <th>Nom</th>
            <th>Date</th>
            <th>reference</th>
            <th>prix achat</th>
            <th>prix vente</th>
            <th>prix vente</th>
            <th>quantite</th>

            <th>Modifier</th>
            <th>Supprimer</th>
        </tr>
        </thead>
        {% for produit in produits %}
            <tr>
                <td>{{ produit.id }}</td>
                <td>{{ produit.nomProduit }}</td>
                <td>{{ produit.dateProduit }}</td>
                <td>{{ produit.reference }}</td>
                <td>{{  produit.prixAchat}}</td>
                <td>{{  produit.prixVente}}</td>
                <td>{{  produit.quantite}}</td>







            </tr>
        {% endfor %}

    </table>

{% endblock %}
